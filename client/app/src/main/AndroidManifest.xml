<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="three.version.project.amourversionfor"
    xmlns:tools="http://schemas.android.com/tools">


    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <supports-screens android:anyDensity="true"
        android:normalScreens="true"
        android:largeScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">

        <activity android:name=".Profile.LoginActivity"
          >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Rating.RatingActivity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".Search.SearchActivity" />
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".Search.SettingsSerch"/>
        <activity android:name=".RegisterActivityTwo"/>
        <activity android:name=".GalleryActivity"/>
        <activity android:name=".Home.HomeActivity"
            android:theme="@style/MyTheme">
        </activity>
        <activity android:name=".UsersPhotoGallery"/>
        <activity android:name=".MessageActivity"/>
        <activity android:name=".SettingsProfile"/>

    </application>

</manifest>